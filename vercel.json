{
  "version": 2,
  "builds": [
    {
      "src": "src",
      "use": "@vercel/next"
    },
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/$1"
    }
  ]
}